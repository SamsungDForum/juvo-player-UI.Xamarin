    <TargetFrameworks>tizen50;tizen60</TargetFrameworks>
    <TizenCreateTpkOnBuild>false</TizenCreateTpkOnBuild>
    <SignAssembly>false</SignAssembly>
    <LangVersion>7.3</LangVersion>
    <Configurations>Debug;Release;Debug HotReload</Configurations>
  </PropertyGroup>

  <ItemGroup>
	  <PackageReference Include="SkiaSharp" Version="1.60.3">
		  <ExcludeAssets>Runtime</ExcludeAssets>
	  </PackageReference>
	  <PackageReference Include="JuvoPlayer" Version="2.3.0-alpha" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
</Project>

